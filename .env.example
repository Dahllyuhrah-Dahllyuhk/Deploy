# Google OAuth2
GOOGLE_CLIENT_ID=
GOOGLE_CLIENT_SECRET=

# Kakao OAuth2
KAKAO_CLIENT_ID=
KAKAO_CLIENT_SECRET=

# MongoDB (Docker 컨테이너 — 변경 불필요)
# SPRING_DATA_MONGODB_URI는 docker-compose.yml에서 자동 주입

# PostgreSQL → AWS RDS
# 형식: jdbc:postgresql://<RDS 엔드포인트>:5432/<DB명>
# 예시: jdbc:postgresql://matuabom.xxxxxx.ap-northeast-2.rds.amazonaws.com:5432/matuabom
POSTGRES_URL=jdbc:postgresql://<RDS_ENDPOINT>:5432/matuabom
POSTGRES_USER=matuabom
POSTGRES_PASSWORD=

# Redis (Docker 컨테이너)
REDIS_PASSWORD=

# JWT (최소 64자 이상의 랜덤 문자열)
# 생성: openssl rand -base64 64
JWT_SECRET=

# Gemini AI
GEMINI_API_KEY=
